using System;

class SumOf5Numbers
{
    static void Main()
    {
        double sum = 0;
        Console.WriteLine("Enter five numbers with space:");
        string[] num = Console.ReadLine().Split();
        for (int i = 0; i < 5; i++)
        {
            double x = Convert.ToDouble(num[i]);
            sum = sum + x;
        }
        Console.WriteLine(sum);
    }
}

