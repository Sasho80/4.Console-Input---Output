using System;

class SumOf5Numbers
{
    static void Main()
    {
        int n =5;
        double sum = 0;
        double []num=new double[n];
        for (int i = 0; i < n; i++)
        {
            num[i] = double.Parse(Console.ReadLine());
            sum = sum + num[i];
        }
        for (int i = 0; i < n; i++)
        {
            Console.Write(num[i]+" ");
        }
        Console.WriteLine(sum);
    }
}

