using System;

class PrintCompanyInformation
{
    static void Main()
    {
        Console.WriteLine("Enter Company name:");
        string companyInfo = Console.ReadLine();
        Console.WriteLine("Enter Company address:");
        string companyAddress = Console.ReadLine();
        Console.WriteLine("Enter Phone number:");
        string phoneNum = Console.ReadLine();
        Console.WriteLine("Enter Fax number:");
        string faxNum = Console.ReadLine();
        Console.WriteLine("Enter Web site:");
        string webSite = Console.ReadLine();
        Console.WriteLine("Enter Menager first name:");
        string firstName = Console.ReadLine();
        Console.WriteLine("Enter Menager last name:");
        string lastName = Console.ReadLine();
        Console.WriteLine("Enter Menager age:");
        string ManagerAge = Console.ReadLine();
        Console.WriteLine("Enter Menager phone:");
        string ManagerPhone = Console.ReadLine();
        Console.WriteLine();
        Console.WriteLine(companyInfo);
        Console.WriteLine("Address:"+companyAddress);
        Console.WriteLine("Tel."+phoneNum);
        Console.WriteLine("Fax:"+faxNum);
        Console.WriteLine("Web site"+webSite);
        Console.Write("Manager:"+ firstName+" "+lastName);
        Console.Write("("+"age: "+ManagerAge);
        Console.Write(" tel. " + ManagerPhone + ")");
        Console.WriteLine();
    }
}

