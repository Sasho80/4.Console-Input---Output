using System;

class SumOfNNumbers
{
    static void Main()
    {
        int n = int.Parse(Console.ReadLine());
        double[] num = new double[n];
        double sum = 0;
        for (int i = 0; i < n; i++)
        {
            num[i]=double.Parse(Console.ReadLine());
            sum = sum + num[i];
        }
        Console.WriteLine(sum);
    }
}

